  "scripts": {
    "postinstall": "lerna bootstrap",
    "format": "prettier './**/*.{json,md,mdx,html}' --write",
    "lint:css": "stylelint ./**/*.scss",
    "lint:es": "eslint . --ext .js,.ts,.tsx,.mdx,.html",
    "lint": "yarn lint:es && yarn lint:css",
    "test": "lerna run test --stream --sort",
    "test:coverage": "lerna run test:coverage --stream --sort",
    "release": "lerna version && yarn build && lerna publish from-package",
    "build": "lerna run build --stream --sort",
    "build:base": "yarn build --scope @mezzanine-form/core",
    "build:clean": "lerna run build:clean --stream --parallel",
    "commit": "git-cz",
    "core:test": "yarn workspace @mezzanine-form/core test",
    "core:test:coverage": "yarn core:test --coverage",
    "react-hook-form:test": "yarn workspace @mezzanine-form/react-hook-form test",
    "react-hook-form:test:coverage": "yarn react-hook-form:test --coverage",
    "react-hook-form:build": "yarn workspace @mezzanine-form/react-hook-form build",
    "react-hook-form:build:all": "yarn build:base --scope @mezzanine-form/react-hook-form",
    "react-hook-form:storybook": "yarn workspace @mezzanine-form/react-hook-form storybook"
  },
